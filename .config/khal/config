[calendars]

[[cal_local]]
path = ~/.local/share/vdirsyncer/calendars/*
type = discover

[locale]
timeformat = %H:%M
dateformat = %d.%m
longdateformat = %d.%m.%Y
datetimeformat = %d.%m %H:%M
longdatetimeformat = %d.%m.%Y %H:%M
firstweekday = 0
weeknumbers = left

[default]
default_calendar = personal
print_new = event
timedelta = 30d
