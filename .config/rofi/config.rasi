configuration {
    modi: "run";
    font: "IBMPlexMono Medium 11";
    cycle: true;
    sort: true;
    matching: "fuzzy";
    dpi: 0;
    scroll-method: 1;
    timeout {
        action: "kb-cancel";
        delay: 0;
     }
    filebrowser {
         directories-first: true;
         sorting-method: "name";
     }
}

@import "colors.rasi"
