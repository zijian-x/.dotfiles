{
    "dialog.rounded": false,
    "notification.border": false,
    "signature.enable": false,
    "coc.source.buffer.enable": false,
    "inlayHint.display": false,
    "coc.preferences.promptInput": false,

    "suggest.minTriggerInputLength": 3,
    "suggest.noselect": true,
    "suggest.virtualText": true,

    "sh.enable": true,
    "json.enable": true,
    "clangd.enabled": true,
    "clangd.disableSnippetCompletion": true,
    "rust-analyzer.server.path": "$XDG_DATA_HOME/rustup/toolchains/stable-x86_64-unknown-linux-gnu/bin/rust-analyzer",
    "rust-analyzer.completion.callable.snippets": "none",
    "java.completion.enabled": true,
    "java.completion.guessMethodArguments": false,
    "java.saveActions.organizeImports": true,
    "java.project.referencedLibraries": ["lib/**/*.jar"],
    "java.import.gradle.offline.enabled": true,
    "java.import.gradle.wrapper.enabled": false,
    "java.import.gradle.home": "/usr/share/java/gradle",
    "java.import.gradle.user.home": "/home/znx/.gradle"
}
